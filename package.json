{
  "name": "turn-based-accounting",
  "version": "2.0.0-new-archi.0",
  "main": "index.js",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "upgrade-interactive": "npm-check --update",
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "start": "remix-serve build"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "4.5.0",
    "@remix-run/node": "1.7.3",
    "@remix-run/react": "1.7.3",
    "@remix-run/serve": "1.7.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@remix-run/dev": "1.7.3",
    "@remix-run/eslint-config": "1.7.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "babel-loader": "8.2.5",
    "esbuild-register": "3.3.3",
    "eslint": "8.26.0",
    "npm-check": "6.0.1",
    "npm-run-all": "4.1.5",
    "prisma": "4.5.0",
    "tailwindcss": "3.2.1",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
